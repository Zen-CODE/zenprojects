{
    "zenplayer_url": "http://9.0.0.2:9001",
    "hotkeymap": {
        "<cmd>+1": {
            "function": "zenplayer",
            "params": ["play_previous"]
        },
        "<cmd>+2": {
            "function": "zenplayer",
            "params": ["play_pause"]
        },
        "<cmd>+3": {
            "function": "zenplayer",
            "params": ["stop"]
        },
        "<cmd>+4": {
            "function": "zenplayer",
            "params": ["play_next"]
        },
        "<cmd>+9": {
            "function": "zenplayer",
            "params": ["volume_down"]
        },
        "<cmd>+0": {
            "function": "zenplayer",
            "params": ["volume_up"]
        },
        "<cmd>+8": {
            "function": "controller",
            "params": ["quit"]
        }
    }
}
